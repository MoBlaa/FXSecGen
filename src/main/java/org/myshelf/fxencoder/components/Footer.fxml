<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.*?>
<?import org.myshelf.fxencoder.components.Footer?>
<fx:root type="org.myshelf.fxencoder.components.Footer" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <Separator prefWidth="200.0" BorderPane.alignment="CENTER">
            <padding>
                <Insets top="20.0" left="30.0" right="30.0" />
            </padding>
        </Separator>
    </top>
    <right>
        <FlowPane alignment="BOTTOM_RIGHT" columnHalignment="RIGHT" hgap="10.0"
                  BorderPane.alignment="CENTER">
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
            </padding>
            <Button fx:id="btnPrevious" mnemonicParsing="false" text="Previous">
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </padding>
            </Button>
            <Button fx:id="btnNext" mnemonicParsing="false" text="Next">
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </padding>
            </Button>
        </FlowPane>
    </right>
    <left>
        <FlowPane alignment="CENTER_LEFT" BorderPane.alignment="CENTER">
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
            </padding>
            <Button fx:id="btnRefresh" mnemonicParsing="false" text="Refresh">
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </padding>
            </Button>
        </FlowPane>
    </left>
</fx:root>
